{
	"control_variables": {
		"packing_fraction": {
			"min": 0.005,
			"max": 0.1
		},
		"polynomial_triso": {
			"order": 3,
			"min": -1,
			"max": 1,
			"radius": 4235e-5, 
			"volume": 10, 
			"slices": 10, 
			"height": 10
		}
	},
	"evaluators": {
		"openmc": {
			"input_script": "openmc_inp.py",
			"inputs": ["packing_fraction", "polynomial_triso"],
			"outputs": ["packing_fraction", "keff"]
		}
	},
	"constraints": {
		"keff": {
			"operator": [">="],
			"constrained_val": [1.0]
		}
	},
	"algorithm": {
		"objective": "min",
		"optimized_variable": "packing_fraction",
		"pop_size": 4,
		"generations": 10,
		"mutation_probability": 0.5, 
		"mating_probability": 0.5, 
		"selection_operator": {
			"operator": "selBest",
			"k": 1
		},
		"mutation_operator": {
			"operator": "mutPolynomialBounded",
			"indpb": 0.5,
			"eta": 0.5
		},
		"mating_operator": {
			"operator": "cxOnePoint"
		}
	}
}
